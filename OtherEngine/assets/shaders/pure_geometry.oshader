vertex [mesh : default] {

  out vec3 pos;
  
  void main() {
    gl_Position = projection * view * models[gl_InstanceID] * vec4(voe_position , 1.0);
    material.ambient = ambient[gl_InstanceID];
  };

}

fragment {
  
  out vec4 FragColor;

  void main () {
    FragColor = material.ambient;
  };

}
